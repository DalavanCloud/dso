@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dso: <http://purl.org/ontology/dso#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssso: <http://purl.org/ontology/ssso#> .
@prefix vann: <http://purl.org/vocab/vann/> .

<http://purl.org/ontology/dso>
    vann:preferredNamespacePrefix "dso" ;
    a owl:Ontology ;
    rdfs:label "DSO", "Document Service Ontology" .

dso:Interloan
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "Interloan" ;
    rdfs:subClassOf dso:Service .

dso:Loan
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "Loan" ;
    rdfs:subClassOf dso:Service .

dso:OpenAccess
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "OpenAccess" ;
    rdfs:subClassOf dso:Service .

dso:Presentation
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "Presentation" ;
    rdfs:subClassOf dso:Service .

dso:Service
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "Service" ;
    rdfs:subClassOf ssso:ServiceEvent .

dso:withDocument
    a owl:ObjectProperty ;
    rdfs:domain dso:Service ;
    rdfs:isDefinedBy <http://purl.org/ontology/dso> ;
    rdfs:label "withDocument" ;
    rdfs:range bibo:Document, foaf:Document ;
    owl:inverseOf ssso:consumedBy .

